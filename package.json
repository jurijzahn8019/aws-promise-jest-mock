{
  "name": "@jurijzahn8019/aws-promise-jest-mock",
  "version": "2.0.0",
  "private": false,
  "description": "simple libraray for jest-tested projects to create jest mock for js aws-sdk  .promise() calls",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "tsc -w",
    "clean": "rm -rf tmp/* dist/*",
    "coveralls": "cat ./tmp/coverage/lcov.info | coveralls",
    "lint": "eslint src/**/*.ts",
    "packlocal": "export P=jurijzahn8019-aws; npm run clean && npm run build && npm pack && mkdir -p ./tmp && mv -f $P-*.tgz ./tmp/ && tar xfz ./tmp/$P-*.tgz -C ./tmp",
    "pub": "npm run clean && npm run build && npm publish",
    "release": "standard-version",
    "test": "jest"
  },
  "keywords": [
    "aws",
    "jest",
    "mock",
    "aws-sdk",
    "promise"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "lint": "eslint src/**/*.ts"
    }
  },
  "author": "Jurij Zahn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurijzahn8019/aws-promise-jest-mock.git"
  },
  "bugs": {
    "url": "https://github.com/jurijzahn8019/aws-promise-jest-mock/issues"
  },
  "homepage": "https://github.com/jurijzahn8019/aws-promise-jest-mock#readme",
  "peerDependencies": {
    "aws-sdk": "^2.x.x",
    "jest": "^24.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.7.0",
    "aws-sdk": "^2.553.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "git-branch": "^2.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "prettier": "^1.18.2",
    "rollup": "^1.26.0",
    "rollup-plugin-typescript2": "^0.24.2",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  }
}
